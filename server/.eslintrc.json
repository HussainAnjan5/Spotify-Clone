{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "single"],
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "comma-dangle": "off"
  }
}
